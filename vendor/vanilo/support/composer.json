{
    "name": "vanilo/support",
    "description": "Common Vanilo functions, traits, helpers",
    "type": "library",
    "license": "MIT",
    "keywords": ["ecommerce", "laravel", "vanilo", "artkonekt"],
    "support": {
        "source": "https://github.com/vanilophp/support"
    },
    "authors": [
        {
            "name": "Attila Fulop",
            "homepage": "https://github.com/fulopattila122"
        }
    ],
    "require": {
        "php": "^7.3|^8.0",
        "illuminate/support": "~6.0|~7.0|~8.0",
        "vanilo/contracts": "^2.1"
    },
    "require-dev": {
        "phpunit/phpunit": "~8.0|~9.0"
    },
    "autoload": {
        "psr-4": {
            "Vanilo\\Support\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Vanilo\\Support\\Tests\\": "tests/"
        }
    },
    "suggest": {
        "spatie/laravel-medialibrary": "Allows associating images (or other files) with Eloquent models (^7.0|^8.0|^9.0)",
        "konekt/concord": "Library to write Modular Applications (^1.5)"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.1.x-dev"
        }
    }
}
