{
    "name": "vanilo/product",
    "description": "Vanilo Product Module",
    "type": "library",
    "license": "MIT",
    "keywords": ["product", "ecommerce", "vanilo", "artkonekt", "laravel"],
    "support": {
        "source": "https://github.com/vanilophp/product"
    },
    "authors": [
        {
            "name": "Attila Fulop",
            "homepage": "https://github.com/fulopattila122"
        }
    ],
    "require": {
        "php": "^7.3|^8.0",
        "konekt/concord": "~1.6",
        "konekt/enum": "~3.0",
        "cviebrock/eloquent-sluggable": "4.3 - 8"
    },
    "require-dev": {
        "phpunit/phpunit": "~8.0|~9.0",
        "orchestra/testbench": "~4.0|~5.0|~6.0"
    },
    "autoload": {
        "psr-4": { "Vanilo\\Product\\": "src/" }
    },
    "autoload-dev": {
        "psr-4": {
            "Vanilo\\Product\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.1.x-dev"
        }
    }
}
