{
    "name": "vanilo/order",
    "description": "Vanilo Order Module",
    "type": "library",
    "license": "MIT",
    "keywords": ["order", "ecommerce", "vanilo", "artkonekt", "laravel"],
    "support": {
        "source": "https://github.com/artkonekt/order"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "authors": [
        {
            "name": "Attila Fulop",
            "homepage": "https://github.com/fulopattila122"
        }
    ],
    "require": {
        "php": "^7.3|^8.0",
        "konekt/concord": "^1.6",
        "konekt/enum": "^3.0",
        "konekt/user": "^2.0",
        "konekt/address": "^2.0",
        "konekt/laravel-migration-compatibility": "~1.1",
        "vanilo/contracts": "^2.1",
        "vanilo/support": "^2.1"
    },
    "require-dev": {
        "phpunit/phpunit": "~8.0|~9.0",
        "orchestra/testbench": "~4.0|~5.0|~6.0",
        "orchestra/database": "~4.0|~5.0|~6.0",
        "laravel/legacy-factories": "^1.0"
    },
    "autoload": {
        "psr-4": { "Vanilo\\Order\\": "src/" }
    },
    "autoload-dev": {
        "psr-4": {
            "Vanilo\\Order\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.2.x-dev"
        }
    }
}
